        $(document).ready(function () {
            $('.dashboard').addClass('active');
            $('.nav-link').click(function () {
                $('.nav-link').removeClass('active');
                $(this).addClass('active');
            });
            $('.dropdown-menu').on('hidden.bs.dropdown', function () {
                $('.nav-link').removeClass('active');
                $('.dashboard').addClass('active');
            });
        });
    </script> *@
</body>
</html>