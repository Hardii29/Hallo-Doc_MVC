@{
    string path = Context.Request.Path;
}
<!DOCTYPE html>
<html lang="en" class="w-100">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/css/intlTelInput.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/intlTelInput.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://kit.fontawesome.com/a07e852713.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.7.1/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <title>@ViewData["Title"]</title>
    <link href="~/css/admin_dashboard.css" rel="stylesheet">
 @*    <link rel="stylesheet" href="~/Hallo_Doc.styles.css" asp-append-version="true" /> *@
 <style>
        .dropdown-menu {
            display: none;
            position: absolute;
            background-color: white;
            min-width: 160px;
            box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2);
            z-index: 1;
        }

        .dropdown:hover .dropdown-menu {
            display: block;
        }

        .nav-link.active {
            background-color: white !important;
            color: #17a2b8 !important;
            border-bottom: 2px solid #17a2b8 !important;
        }
 </style>
</head>
<body >
    <header>
        
    <nav class="navbar navbar-expand-lg navbar-toggler shadow mb-2 bg-body">
            <div class="container-fluid flex-column">
            <ul class="navbar-nav flex-row w-100 justify-content-between">
                    <button class="navbar-toggler navbar-light nav-tog-btn px-0 mb-2 mt-5" type="button"
                            data-bs-toggle="offcanvas" data-bs-target="#navbarSupportedContent">
                        <i class="fa-solid fa-bars"></i>
                    </button>
                <li class="nav-item">
                    <img class="img-fluid m-2" src="./../images/logo.png" alt="" width="150" height="70">
                </li>

                <li class="nav-item navbar-right d-flex flex-row ml-auto mt-5 h-25">

                    <span class="dblock mt-2">Welcome <span class="fw-bold">@Model.AdminName</span></span>

                        <a class="btn btn-outline-info mx-2 h-25 dblock" asp-controller="AdminLogin" asp-action="Logout">Logout</a>

                        <a class="btn btn-outline-info mx-2 dnone" asp-controller="AdminLogin" asp-action="Logout">
                        <i class="fa fa-right-from-bracket"></i>
                    </a>

                    <button type="button" class="btn btn-outline-info moon-img" onclick="myFunctions()">
                        <i class="fa fa-moon"></i>
                    </button>

                </li>
            </ul>

                <hr class="border border-dark border-1 w-100 m-0 dblock">

                <div class="w-50 dash-nav offcanvas offcanvas-start mr-auto" id="navbarSupportedContent">
                    <ul class="navbar-nav nav-underline offcanvas-body">

                    <li class="nav-item mx-2 d-flex">
                            <a class="nav-link dashboard @(path.StartsWith("/Admin/Admin_dashboard") || path.EndsWith("CreateRequest") || path.EndsWith("View_upload") || path.EndsWith("View_notes") || path.EndsWith("View_case") || path.EndsWith("Order") ? "active" : "")" asp-controller="Admin" asp-action="Admin_dashboard">Dashboard</a>
                    </li>
                    <li class="nav-item mx-2">
                            <a class="nav-link @(path.StartsWith("/AdminNavbar/ProviderLocation") ? "active" : "")" asp-controller="AdminNavbar" asp-action="ProviderLocation">Provider Location</a>
                    </li>
                    <li class="nav-item mx-2">
                            <a class="nav-link @(path.StartsWith("/Admin/AdminProfile") ? "active" : "")" asp-controller="Admin" asp-action="AdminProfile" asp-route-adminId="@Model.AdminId">My Profile</a>
                    </li>
                        <li class="nav-item mx-2 dropdown">
                            <a class="nav-link @(path.StartsWith("/AdminNavbar/ProviderMenu") || path.EndsWith("Scheduling") || path.EndsWith("MDsOnCall") || path.EndsWith("RequestedShift") || path.EndsWith("CreateProvider") ? "active" : "")">Providers</a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="ProviderMenu">Provider</a>
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="Scheduling">Scheduling</a>
                                <a class="dropdown-item" href="#">Invoicing</a>
                            </div>
                        </li>
                    <li class="nav-item mx-2">
                            <a class="nav-link @(path.StartsWith("/AdminNavbar/VendorMenu") ? "active" : "")" asp-controller="AdminNavbar" asp-action="VendorMenu">Partners</a>
                    </li>
                        <li class="nav-item mx-2 dropdown">
                            <a class="nav-link @(path.StartsWith("/AdminNavbar/AccountAccess") || path.StartsWith("/AdminNavbar/UserAccess") || path.EndsWith("CreateAdmin") ? "active" : "")">Access</a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="AccountAccess">Account Access</a>
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="UserAccess">User Access</a>
                            </div>
                        </li>
                        <li class="nav-item mx-2 dropdown">
                            <a class="nav-link @(path.EndsWith("SearchRecords") || path.EndsWith("EmailLogs") || path.EndsWith("SMSLogs") || path.EndsWith("PatientRecords") || path.EndsWith("PatientHistory") || path.EndsWith("BlockHistory") ? "active" : "")">Records</a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="SearchRecords">Search Records</a>
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="EmailLogs">Email Logs</a>
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="SMSLogs">SMS Logs</a>
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="PatientHistory">Patient Records</a>
                                <a class="dropdown-item" asp-controller="AdminNavbar" asp-action="BlockHistory">Blocked History</a>
                            </div>
                    </li>
                </ul>
          </div>
            </div>
    </nav>
        <script type="text/javascript" src='https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.8.3.min.js'></script>
        <script type="text/javascript" src='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.0.3/js/bootstrap.min.js'></script>

    </header>

    @RenderBody()
    <script src="~/js/Custom.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
   <script src="~/js/site.js" asp-append-version="true"></script>
    
    @await RenderSectionAsync("Scripts", required: false)
    @await Component.InvokeAsync("Notyf")
