@model Hallo_Doc.Entity.ViewModel.BlockHistory;
@{
    ViewData["Title"] = "Patient Record";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<body class="bg-light">
    <div class="flex-row d-flex mt-5 m-3">
        <h4 style="font-weight:bold;">Patient Record</h4>
    </div>
    <div class="container-custom mt-3">
        
        <div class="table-responsive">
            <table class="table table-hover mt-3" style="width:100%">
                <thead class="table-light">
                    <tr style="padding:10px;">
                        <th class="col-2"></th>
                        <th class="col-2">Phone Number</th>
                        <th class="col-2">Email</th>
                        <th class="col-2">Created Date</th>
                        <th class="col-2">Notes</th>
                        <th class="col-1">Is Active</th>
                        <th class="col-1">Action</th>
                    </tr>
                </thead>
                <tbody id="blockHistory">
                    
                        <tr class=" mt-2">
                            <td class="col-2"></td>
                            <td class="col-2"></td>
                            <td class="col-2"></td>
                            <td class="col-2"></td>
                            <td class="col-2"></td>
                            <td class="col-1">
                            </td>
                            <td class="col-1"><a class="btn btn-outline-info btn-sm">Unblock</a></td>
                        </tr>
                    
                </tbody>
            </table>
        </div>
    </div>
</body>
